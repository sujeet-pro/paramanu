{
  "font": {
    "family": {
      "$type": "fontFamily",
      "sans": { "$value": "\"Roboto\", \"Noto Sans\", system-ui, -apple-system, sans-serif" }
    },
    "size": {
      "$type": "dimension",
      "xs": { "$value": "0.6875rem" },
      "sm": { "$value": "0.75rem" },
      "md": { "$value": "0.875rem" },
      "lg": { "$value": "1rem" },
      "xl": { "$value": "1.375rem" },
      "2xl": { "$value": "1.75rem" },
      "3xl": { "$value": "2.25rem" }
    },
    "weight": {
      "$type": "fontWeight",
      "medium": { "$value": "500" }
    }
  }
}
