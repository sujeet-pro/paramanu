{
  "radius": {
    "$type": "dimension",
    "none": { "$value": "0px" },
    "sm": { "$value": "8px" },
    "md": { "$value": "12px" },
    "lg": { "$value": "16px" },
    "xl": { "$value": "28px" },
    "full": { "$value": "9999px" }
  },
  "shadow": {
    "$type": "shadow",
    "sm": { "$value": "0 1px 3px 1px rgb(0 0 0 / 0.15), 0 1px 2px 0 rgb(0 0 0 / 0.3)" },
    "md": { "$value": "0 2px 6px 2px rgb(0 0 0 / 0.15), 0 1px 2px 0 rgb(0 0 0 / 0.3)" },
    "lg": { "$value": "0 4px 8px 3px rgb(0 0 0 / 0.15), 0 1px 3px 0 rgb(0 0 0 / 0.3)" }
  }
}
